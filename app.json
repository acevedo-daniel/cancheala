{
  "expo": {
    "name": "Cancheala",
    "slug": "cancheala",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/iconSplash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/backgrounds/background.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#00C853"
    },
    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/iconSplash.png",
        "backgroundColor": "#00C853"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.cancheala"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "cancheala",
    "plugins": ["expo-router", "expo-font"]
  }
}
