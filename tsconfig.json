{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@lib/*": ["lib/*"],
      "@assets/*": ["assets/*"],
      "@types/*": ["types/*"],
      "@stores/*": ["stores/*"]
    }
  }
}
